<?xml version='1.0' encoding='utf-8'?>
<widget id="com.kurohai.hellodexcom" version="0.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HelloDexcom</name>
    <description>
        A bluetooth app thing.
    </description>
    <author email="kurohai@gmail.com" href="http://kurohai.com">Kurohai Creations</author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-ble-central" spec="1" />
    <plugin name="cordova-plugin-compat" spec="1" />
    <plugin name="cordova-plugin-bluetoothle" spec="1" />
    <plugin name="cordova-plugin-file" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
